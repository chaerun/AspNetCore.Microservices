{
  "ApiSettings": {
    "UnitUrl": "https://unit-api",
    "EmployeeUrl": "https://employee-api"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithClientIp",
      "WithClientAgent"
    ],
    "WriteTo": [
      "Console"
    ]
  },
  "AllowedHosts": "*",
  "IdentityServerUrl": "https://identity-api"
}
